<resources>

    <style name="Theme.MultiBackStack" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="actionBarStyle">@style/Theme.MultiBackStack.ActionBar</item>
    </style>

    <style name="Theme.MultiBackStack.ActionBar" parent="@style/Widget.AppCompat.ActionBar.Solid">
        <item name="elevation">4dp</item>
    </style>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@android:color/holo_blue_bright</item>
        <item name="colorPrimaryDark">@android:color/background_dark</item>
        <item name="colorAccent">@android:color/background_dark</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:colorForeground">@android:color/holo_blue_light</item>
        <item name="android:colorBackground">@android:color/background_light</item>
        <item name="android:textColorPrimary">@android:color/black</item>
        <item name="android:textColorPrimaryInverse">@android:color/black</item>
    </style>

    <style name="XNKT" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:colorForeground">@android:color/background_light</item>
    </style>
</resources>